{
  "extends": [
    "4catalyzer-react",
    "4catalyzer-jest",
    "prettier",
    "4catalyzer-typescript"
  ],
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "prettier/prettier": "error",
    "@typescript-eslint/naming-convention": "off"
  }
}
